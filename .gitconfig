[user]
	email = joshishantanu4@gmail.com
	name = Shantanu
[credential]
	helper = osxkeychain
[core]
	excludesfile = /Users/shantanu/.gitignore_global
	editor = nvim
[alias]
	st = status
	co = checkout
	ci = commit -m 
	ca = commit --amend -m 
	last = log -1 HEAD
	merge = merge --no-ff 
	push-this = push -u origin HEAD
[pager]
	branch = false
[url "git@github.com:"]
	insteadOf = https://github.com/
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
